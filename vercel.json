{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "DB_USER": "@db-user",
    "DB_PASSWORD": "@db-password",
    "DB_HOST": "@db-host",
    "DB_NAME": "@db-name",
    "DB_PORT": "@db-port"
  }
}
